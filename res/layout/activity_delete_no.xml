<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="fill_parent"
    android:layout_height="fill_parent" android:id="@+id/MainLayout"
    >
    
    <TextView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Find My Phone BETA!
currently in progress on this page, testing if jumps to this page and back"
        android:id="@+id/textView"
        android:layout_gravity="center_horizontal" />
    
    <EditText
        android:id="@+id/editText3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="numberSigned|numberDecimal"         
        android:hint="Enter a number..."/>
    
    <Button 
	    android:id="@+id/button5"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:onClick="ADDNUMBER"
	    android:text="Press Add entered in number"/>
    
    <Button 
	    android:id="@+id/button6"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:onClick="DELNUMBER"
	    android:text="Press to Delete Entered in number"/>
    
    <Button 
	    android:id="@+id/button4"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:onClick="GOBACK"
	    android:text="Press to return to main screen"/>
    
    <TextView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Click to change PassPhrase sent"
        android:id="@+id/textView2"
        android:layout_gravity="center_horizontal" />
    
    <ListView
	    android:id="@+id/listView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:clickable="true"/>
        
	    
    
    
</LinearLayout>

